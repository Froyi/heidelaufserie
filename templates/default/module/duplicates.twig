<div class="duplicates-page js-duplicates-page">
    {% if duplicatesByDefault %}
        <div class="runner-container">
            {% for runnerDuplicate in duplicatesByDefault %}
                <div class="runner-data"  style="background-color: green;">
                    {{ runnerDuplicate.runner.getFirstname }} {{ runnerDuplicate.runner.getSurname }} {{ runnerDuplicate.runner.getAgeGroup.getBirthYear }}
                </div>
                {% for runner in runnerDuplicate.duplicates %}
                    <div>
                        {{ runner.getFirstname }} {{ runner.getSurname }} {{ runner.getAgeGroup.getBirthYear }}
                    </div>
                {% endfor %}
            {% endfor %}
        </div>
    {% endif %}
</div>

